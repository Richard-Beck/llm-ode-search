{
  "model": {
    "name": "growth_model_refined",
    "states": [
      "X",
      "S",
      "Q"
    ],
    "params": [
      "mu_max",
      "Ks",
      "m",
      "Q0",
      "k_Q"
    ],
    "ode_text": "rho = mu_max * S / (Ks + S);\nmu = mu_max * (1 - Q0 / Q);\nd/dt(X) = (mu - m) * X;\nd/dt(S) = -rho;\nd/dt(Q) = k_Q * mu * X;"
  },
  "init": {
    "X": 0.05,
    "S": 5,
    "Q": 1
  },
  "param_values": {
    "mu_max": 1.5,
    "Ks": 1,
    "m": 0.1,
    "Q0": 0.8,
    "k_Q": 0.01
  },
  "inputs": [],
  "observe": {
    "X": "X"
  },
  "noise": [
    {
      "target": "X",
      "type": "gaussian",
      "sigma": 0.1
    }
  ],
  "sampling": {
    "t0": 0,
    "t1": 48,
    "dt": 8
  },
  "replicates": 1,
  "conditions": [
