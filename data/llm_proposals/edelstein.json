{
  "model": {
    "name": "simple_activation_model",
    "states": [
      "Rstar",
      "S"
    ],
    "params": [
      "rho_max",
      "Ks",
      "a0",
      "a1",
      "mu_max",
      "m"
    ],
    "ode_text": "rho = rho_max*S/(Ks+S);\nmu = mu_max*(1 - Rstar/(a0 + a1*S));\nd/dt(S) = -rho*S;\nd/dt(Rstar) = (mu - m)*Rstar;"
  },
  "init": {
    "Rstar": 0.0168,
    "S": 1
  },
  "param_values": {
    "rho_max": 0.5,
    "Ks": 0.1,
    "a0": 0.02,
    "a1": 0.5,
    "mu_max": 0.8,
    "m": 0.05
  },
  "inputs": [
    {
      "name": "L",
      "type": "piecewise",
      "schedule": [
        [
          0,
          2,
          0
        ],
        [
          2,
          6,
          1
        ],
        [
          6,
          14,
          0
        ],
        [
          14,
          18,
          0.5
        ],
        [
          18,
          25,
          0
        ]
      ]
    }
  ],
  "observe": {
    "Rstar": "Rstar"
  },
  "noise": [
    {
      "target": "Rstar",
      "type": "gaussian",
      "sigma": 0.02
    }
  ],
  "sampling": {
    "t0": 0,
    "t1": 25,
    "dt": 1
  },
  "replicates": 1,
  "conditions": [
