{
  "model": {
    "name": "delayed_oscillator_model",
    "states": [
      "N",
      "P",
      "N_delay"
    ],
    "params": [
      "k1",
      "k2",
      "k3",
      "k4",
      "tau"
    ],
    "ode_text": "d/dt(N) = k1*P - k2*N;\nd/dt(P) = k3*N_delay - k4*P;\nd/dt(N_delay) = (N - N_delay)/tau;"
  },
  "init": {
    "N": 6,
    "P": 1.58,
    "N_delay": 6
  },
  "param_values": {
    "k1": 0.5,
    "k2": 0.1,
    "k3": 0.3,
    "k4": 0.2,
    "tau": 1
  },
  "inputs": [],
  "observe": {
    "N": "N",
    "P": "P"
  },
  "noise": [
    {
      "target": "N",
      "type": "gaussian",
      "sigma": 1
    },
    {
      "target": "P",
      "type": "gaussian",
      "sigma": 0.1
    }
  ],
  "sampling": {
    "t0": 0,
    "t1": 40,
    "dt": 1
  },
  "replicates": 10,
  "conditions": [
    {
      "name": "damped"
    },
    {
      "name": "oscillatory"
    }
  ]
}
